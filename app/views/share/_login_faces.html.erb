<% if @users.present? %>
<aside class="faces">
  <section>
  <h1><%=t 'musick.faces.recently_login'%></h1>
  <ul>
    <% @users.each do |user| %>
    <li><%=link_to image_tag(user.image), users_home_path(user.id), 
      'data-tooltip' => (time_ago_in_words user.current_sign_in_at, true) + t('before') + "\n" + t('musick.signed_in_before'),
      :class => :with_tooltip%>
    </li>
    <% end %>
  </ul>
  </section>
</aside>
<% end %>